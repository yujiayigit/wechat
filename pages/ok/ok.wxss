.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9;
  color: #fff;
}

.commodity_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 10;
  background: rgb(255, 255, 255);
  padding-top: 20rpx;
  height: 200px;
}
.commodity_attr_box{
  text-align: center;
  padding-top: 100rpx;
}